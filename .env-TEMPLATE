APP_PORT = '5000'

# MySQL information and credentials
DB_HOST = 'localhost'
DB_USER = 'root'
DB_PORT = '3306'
DB_PASSWORD = 'root'

# Database to be used
DB_DATABASE = 'test'

# This is necessary in some cases, but leave blank if MySQL error
DB_SOCKETPATH = '/Applications/MAMP/tmp/mysql/mysql.sock'

# 'console.log' for logging database operations, 'false' for not
DB_LOGGING = 'false'

# 'debug_data' for development, remove line for env default
CDN_PATH = 'debug_data'
COOKIE_DOMAIN = 'localhost'

# This is used on Registration and on Requests to stop bots
# Leave as 'key' to skip those features
# Or make your keys here
# https://developers.google.com/recaptcha/intro
GOOGLE_CAPTCHA_V2_PRIVATE_KEY = 'key'
GOOGLE_CAPTCHA_V3_PRIVATE_KEY = 'key'

# To be used on requests, skips some checks
DEBUG_KEY = 'YmFzZTY0IGVuY29kaW5nIGdvIGJycnJy'

